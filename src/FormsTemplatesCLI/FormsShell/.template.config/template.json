{
    "$schema": "https://json.schemastore.org/template",
    "author": "Vijay Anand E G",
    "defaultName": "AppShell",
    "classifications": [
        "Android",
        "iOS",
        "UWP",
        "Windows",
        "Xamarin.Forms",
        "Xaml"
    ],
    "identity": "VijayAnand.FormsShell",
    "groupIdentity": "VijayAnand.FormsTemplates.Shell",
    "description": "An item template for Xamarin.Forms ShellPage in XAML",
    "name": "Xamarin.Forms ShellPage",
    "shortName": "forms-shell",
    "sourceName": "FormsShell",
    "primaryOutputs": [
        {
            "path": "FormsShell.xaml"
        },
        {
            "path": "FormsShell.xaml.cs"
        }
    ],
    "tags": {
        "language": "C#",
        "type": "item"
    },
    "symbols": {
        "namespace": {
            "type": "parameter",
            "replaces": "MyApp.Namespace",
            "datatype": "text",
            "description": "namespace for the generated code"
        }
    }
}
